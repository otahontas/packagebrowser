{
  "name": "packagebrowser",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "build:client": "tsc --noEmit && vite build src/client",
    "build:server": "tsc",
    "dev": "concurrently 'npm run dev:client' 'npm run dev:server'",
    "dev:client": "vite serve src/client",
    "dev:server": "ts-node-dev src/server/index.ts",
    "production:server": "node build/server/index.js",
    "tsc": "tsc",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,json,md}": "prettier --write",
    "src/**/*.{ts,tsx}": "eslint",
    "src/client/**/*": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-react": "^1.0.9",
    "concurrently": "^6.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.22.1",
    "axios": "^0.24.0",
    "fastify": "^3.24.0",
    "fastify-plugin": "^3.0.0",
    "fastify-static": "^4.5.0",
    "got": "^11.8.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.33.4",
    "react-router-dom": "^6.0.2",
    "styled-components": "^5.3.3"
  }
}
